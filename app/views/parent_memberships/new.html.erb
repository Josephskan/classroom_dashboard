<div class="custom-standard-view">
  <h1>New Parent Membership to <%= @classroom.classroom_name %></h1>
  <p><%= link_to "Go back to classroom list", classrooms_path %></p>

  <%= form_for [Classroom.find(params[:classroom_id]), @parent_membership] do |f| %>
    <div>
      <%= f.label :passcode %><br>
    <input type="number"  name="passcode">
    </div>
    <div>
      <%= f.submit "Submit" %> 
    </div>
  <% end %>
</div>